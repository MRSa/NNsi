FORM ID FRMID_MESSAGE AT (0 0 160 160) FRAME SAVEBEHIND MENUID MNUID_MESSAGE
  BEGIN
    TITLE "1:xxxx "
    BUTTON "" ID BTNID_WRITE_MESSAGE  AT (129 145 30 14) USABLE NOFRAME GRAPHICAL BITMAPID BMP_WRITEBUTTON
    BUTTON "" ID BTNID_BACK_MESSAGE   AT (113 145 16 14) USABLE NOFRAME GRAPHICAL BITMAPID BMP_RETURN
    BUTTON "" ID BTNID_JUMP           AT (94  145 19 14) USABLE NOFRAME GRAPHICAL BITMAPID BMP_JUMP
    FIELD ID FLDID_MESSAGE_NUMBER     AT (71  147 17 13) USABLE MAXCHARS 3 NUMERIC UNDERLINED
    BUTTON "" ID BTNID_BOTTOM         AT (59  145 12 14) USABLE NOFRAME GRAPHICAL BITMAPID BMP_BOTTOM
    BUTTON "" ID BTNID_NEXT_MESSAGE   AT (44  145 12 14) USABLE NOFRAME GRAPHICAL BITMAPID BMP_NEXT
    BUTTON "" ID BTNID_PREV_MESSAGE   AT (29  145 12 14) USABLE NOFRAME GRAPHICAL BITMAPID BMP_PREV
    BUTTON "" ID BTNID_TOP            AT (14  145 12 14) USABLE NOFRAME GRAPHICAL BITMAPID BMP_TOP
    BUTTON "" ID BTNID_BOOKMARK       AT (36    0 12 12) USABLE NOFRAME GRAPHICAL BITMAPID BMP_BOOKMARK
    CHECKBOX "" ID CHKID_FAVOR_THREAD AT (0 145 12 AUTO) USABLE
    FIELD ID FLDID_MESSAGE AT (0 17 153 128) USABLE HASSCROLLBAR UNDERLINED MULTIPLELINES LEFTALIGN NONEDITABLE MAXCHARS 32000
    SCROLLBAR ID SCLID_MESSAGE  AT (154 20 7 120) USABLE VALUE 0 MINVALUE 0 MAXVALUE 100 PAGESIZE 11
    SELECTORTRIGGER "( TITLE )" ID SELTRID_THREAD_TITLE AT (50 1 110 11) USABLE LEFTANCHOR
  END

MENU ID MNUID_MESSAGE
  BEGIN
    PULLDOWN "スレ"
      BEGIN
        MENUITEM "情報"                MNUID_THREAD_INFO            "I"
        MENUITEM "先頭へ"              MNUID_TOP_MESSAGE            "T"
        MENUITEM "末尾へ"              MNUID_BOTTOM_MESSAGE         "B"
        MENUITEM "書き込み"            MNUID_WRITE_MESSAGE          "W"
        MENUITEM "差分取得"            MNUID_GET_PART               "A"
      END

    PULLDOWN "編集"
      BEGIN
        MENUITEM "コピー"              MNUID_EDIT_COPY              "C"
        MENUITEM "すべて選択"          MNUID_SELECT_ALL             "S"
        MENUITEM "選択文字列をブラウザで" MNUID_OS5_LAUNCH_WEB      "9"
      END

    PULLDOWN "その他"
      BEGIN
        MENUITEM "一覧画面へ戻る"       MNUID_THREAD_SELECTION       "Z"
        MENUITEM "フォント変更"         MNUID_FONT_MSGVIEW           "0"
        MENUITEM "描画モード変更"       MNUID_CLIE_DRAW_MODE         "2"
        MENUITEM "画面の回転(HandEra)"  MNUID_HANDERA_ROTATE_MSGVIEW "1"
        MENUITEM "回線切断"             MNUID_NET_DISCONN            "Q"
      END
  END

