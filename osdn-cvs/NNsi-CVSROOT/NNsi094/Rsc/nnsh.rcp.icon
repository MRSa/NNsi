    CHECKBOX "一部機種のVFS制限に対応"     ID CHKID_VFS_WORKAROUND AT (20 115 AUTO AUTO) USABLE
    CHECKBOX "OFFLINEスレをVFSに置く"      ID CHKID_VFSUSE_OFFLINE AT (20 125 AUTO AUTO) USABLE
    CHECKBOX "NNsi終了時VFSにDBを保存"     ID CHKID_VFSUSE_DBBACKUP AT (20 135 AUTO AUTO) USABLE
    CHECKBOX "(DEBUG)"                     ID CHKID_DEBUG_DIALOG   AT (95 150 AUTO AUTO) USABLE
    BUTTON   "OK"                          ID BTNID_CONFIG_NNSH_OK     AT ( 5 149 35 10) USABLE FRAME
    BUTTON   "Cancel"                      ID BTNID_CONFIG_NNSH_CANCEL AT (45 149 35 10) USABLE FRAME
    LABEL           "1" ID NNSHSET_1 AT (117 0)      USABLE
    SELECTORTRIGGER "2" ID NNSHSET_2 AT (125 1 9 10) USABLE LEFTANCHOR
    SELECTORTRIGGER "3" ID NNSHSET_3 AT (135 1 9 10) USABLE LEFTANCHOR
    SELECTORTRIGGER "4" ID NNSHSET_4 AT (145 1 9 10) USABLE LEFTANCHOR
  END

